/* plugin/pages/nav/nav.wxss */
page{
  height: 100%;
  overflow: hidden;
  background-color: #000;
}
.page{
  width: 100%;
  height: 100%;
}
.mask{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 99;
}
.mask_floor{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 99;
}
.popup_shop{
  position: absolute;
  width: 100%;
  height: 986rpx;
  background: #FFFFFF;
  box-shadow: 0rpx -8rpx 12rpx 6rpx rgba(0,0,0,0.08);
  border-radius: 16rpx 16rpx 0rpx 0rpx;
  left: 0;
  bottom: -812rpx;
  z-index: 100;
  transition: bottom 0.4s;
}
.popup_shop_unfold{
  bottom: 0;
}
.operate_bar{
  width: 100%;
  height: 48rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.packup_icon{
  width: 58rpx;
  height: 16rpx;
}
.unfold_icon{
  width: 58rpx;
  height: 6rpx;
}
.input_content{
  padding: 0 32rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.input_view{
  position: relative;
  width: 686rpx;
  height: 96rpx;
  background-color: #F0F0F3;
  border-radius: 16rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.input_view_building{
  width: 446rpx;
}
.input_view_search{
  width: 590rpx;
}
.search_icon{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: 50%;
  transform: translateY(-50%);
  left: 32rpx;
}
.search_disabled_icon{
  opacity: 0.5;
}
.search_input{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 0 32rpx 0 96rpx;
}
.input_view_search .search_input{
  padding-right: 80rpx;
}
.search_cancel{
  font-size: 32rpx;
  font-weight: 400;
  color: #444253;
}
.search_close_icon{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: 50%;
  transform: translateY(-50%);
  right: 32rpx;
}
.building_picker{
  position: relative;
  width: 208rpx;
  height: 96rpx;
  border-radius: 8px;
  border: 2rpx solid #F0F0F3;
  font-size: 32rpx;
  font-weight: 400;
  color: #444253;
}
.building_picker picker{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
.picker_view{
  width: 100%;
  height: 96rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  padding: 0 64rpx 0 16rpx;
  overflow:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.picker_arrow{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: 50%;
  transform: translateY(-50%);
  right: 16rpx;
}
.first_sort{
  padding: 0 108rpx;
  height: 128rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.first_sort_item{
  font-size: 36rpx;
  font-weight: 400;
  color: rgba(68, 66, 83, 0.5);
  display: flex;
  align-items: center;
}
.first_sort_item image{
  width: 64rpx;
  height: 64rpx;
  margin-right: 16rpx;
  opacity: 0.5;
}
.first_sort_item_else{
  color: #141A27;
}
.first_sort_item_else image{
  opacity: 1;
}
.first_sort2{
  padding: 0 32rpx;
  height: 128rpx;
  display: flex;
  align-items: center;
  flex-wrap: nowrap;
  overflow-x: scroll;
  border-bottom: 2rpx solid #F0F0F3;
  box-sizing: border-box;
}
.first_sort2_item{
  flex-shrink: 0;
  height: 64rpx;
  padding: 0 28rpx;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(68, 66, 83, 0.5);
}
.first_sort2_item_active{
  background-color: #444253;
  color: #fff;
  border-radius: 8px;
}
.shop_main{
  width: 100%;
  height: 714rpx;
  display: flex;
}
.floor_view{
  width: 128rpx;
  height: 100%;
  overflow-y: scroll;
  background: #F0F0F3;
  box-shadow: inset -8rpx 0px 8rpx 0px rgba(130,130,130,0.05);
  color: #141A27;
  font-size: 32rpx;
  font-weight: 400;
}
.floor_item{
  width: 100%;
  height: 96rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.floor_active{
  background-color: #C4A872;
  color: #fff;
}
.shop_content{
  width: 622rpx;
  height: 100%;
}
.second_sort{
  width: 590rpx;
  height: 96rpx;
  margin-left: 32rpx;
  box-sizing: border-box;
  overflow-x: scroll;
  border-bottom: 2rpx solid #F0F0F3;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
}
.second_sort_item{
  min-width: 96rpx;
  height: 48rpx;
  border-radius: 24rpx;
  background-color: #F0F0F3;
  color: #141A27;
  font-size: 24rpx;
  font-weight: 400;
  display: flex;
  align-items: center;
  justify-content: center;
  white-space: nowrap;
  overflow: hidden;
  margin-right: 16rpx;
  flex-shrink: 0;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.second_sort_active{
  background: #444253;
  color: #fff;
}
.shop_list{
  width: 590rpx;
  height: 618rpx;
  overflow-y: scroll;
  margin-left: 32rpx;
}
.shop_item{
  height: 128rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  border-bottom: 2rpx solid rgba(240, 240, 243, 0.5);
}
.shop_img{
  width: 96rpx;
  height: 96rpx;
  border: 2rpx solid #F0F0F3;
  box-sizing: border-box;
  margin-right: 18rpx;
}
.shop_info{
  width: 366rpx;
}
.shop_title{
  font-size: 32rpx;
  font-weight: 600;
  color: #141A27;
  margin-bottom: 16rpx;
}
.shop_attr{
  color: rgba(68, 66, 83, 0.5);
  font-size: 24rpx;
  font-weight: 400;
  display: flex;
}
.shop_nav{
  width: 96rpx;
  height: 96rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.shop_nav_icon{
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 8rpx;
}
.shop_nav_text{
  font-size: 16rpx;
  font-weight: 400;
  color: #C79E53;
}
.shop_none{
  padding: 120rpx 0;
  text-align: center;
  font-size: 32rpx;
  color: rgba(68, 66, 83, 0.5);
}
.popup_floor{
  position: absolute;
  width: 100%;
  height: 736rpx;
  background: #FFFFFF;
  box-shadow: 0rpx -8rpx 12rpx 6rpx rgba(0,0,0,0.08);
  border-radius: 16rpx 16rpx 0rpx 0rpx;
  left: 0;
  bottom: -750rpx;
  z-index: 100;
  transition: bottom 0.4s;
}
.popup_floor_unfold{
  bottom: 0;
}
.popup_floor_title{
  text-align: center;
  color: #141A27;
  font-size: 36rpx;
  font-weight: 600;
}
.popup_floor_title view:first-child{
  margin-bottom: 20rpx;
}
.floor_picker{
  position: relative;
  height: 352rpx;
  width: 192rpx;
  margin: 32rpx auto;
}
.popup_floor_btn{
  width: 480rpx;
  height: 96rpx;
  border-radius: 16rpx;
  background-color: #C4A872;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: 600;
  color: #fff;
  margin: 0 auto;
}
.picker_center{
  position: absolute;
  width: 100%;
  height: 40px;
  border-radius: 16rpx;
  background-color:#EFECDF;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
}
.picker{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
}
.picker_item{
  font-size: 32rpx;
  font-weight: 400;
  color: #141A27;
  text-align: center;
}
.abnormal_layer{
  position: absolute;
  left: 24rpx;
  top: 20rpx;
  z-index: 130;
}
.abnormal_item{
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-weight: 400;
  color: #FFFFFF;
  text-shadow: 0px 0px 8rpx rgba(0,0,0,0.3);
}
.abnormal_item image{
  width: 48rpx;
  height: 48rpx;
  margin-right: 20rpx;
  margin-top: 2rpx;
}
.abnormal_animation{
  animation: twinkle 2s infinite linear;
  -webkit-animation: twinkle 2s infinite linear;
}
@keyframes twinkle {
  0% {
    opacity: 0.4;
  }
  50% {
    opacity: 1;
  }
  100% {
    opacity: 0.4;
  }
}
@-webkit-keyframes twinkle {
  0% {
    opacity: 0.4;
  }
  50% {
    opacity: 1;
  }
  100% {
    opacity: 0.4;
  }
}
.bylift_remind{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 130;
}
.layer_close_btn{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: 48rpx;
  right: 48rpx;
}
.bylift_content{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.bylift_title{
  text-align: center;
  font-size: 48rpx;
  font-weight: 600;
  color: #fff;
}
.bylift_img{
  display: block;
  width: 578rpx;
  height: 358rpx;
  margin: -48rpx auto 48rpx;
}
.bylift_tip{
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #FFFFFF;
  margin-bottom: 32rpx;
}
.bylift_btn{
  width: 392rpx;
  height: 96rpx;
  background-color: #fff;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: 600;
  color: #141A27;
  margin: 0 auto;
}
.scan_title{
  text-align: center;
  color: #fff;
  font-size: 48rpx;
  font-weight: 500;
  margin-bottom: 32rpx;
}
.locate_fail_btn{
  width: 320rpx;
  height: 96rpx;
  background-color: #fff;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: 600;
  color: #141A27;
  margin: 0 auto;
}
.locate_fail_btn:last-child{
  margin-top: 32rpx;
}
.erect_img{
  width: 578rpx;
  height: 358rpx;
  margin: 0 auto 48rpx;
}
.erect_tip{
  font-size: 28rpx;
  color: #fff;
  line-height: 56rpx;
  text-align: center;
}
.popup_info{
  position: absolute;
  width: 710rpx;
  height: 120rpx;
  left: 50%;
  transform: translateX(-50%);
  bottom: 20rpx;
  border-radius: 16rpx;
  background-color: #fff;
  z-index: 120;
}
.nav_info{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}
.nav_name{
  width: 160rpx;
  text-align: center;
  color: #141A27;
  font-size: 32rpx;
  font-weight: 600;
}
.nav_detail{
  width: 420rpx;
  height: 80rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #444253;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  border-left: 2rpx solid #EDEDED;
  border-right: 2rpx solid #EDEDED;
}
.arrive_txt{
  font-weight: 400;
}
.nav_close_icon{
  width: 160rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav_close_icon image{
  width: 32rpx;
  height: 32rpx;
}
.nav_exit{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}
.exit_title, .exit_cancel{
  width: 50%;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}
.exit_title{
  border-right: 2rpx solid #EDEDED;
  font-size: 36rpx;
  font-weight: 600;
  color: #B90909;
}
.exit_cancel{
  font-size: 36rpx;
  font-weight: 600;
  color: #444253;
}
.current_floor{
  position: absolute;
  width: 98rpx;
  height: 98rpx;
  background-color: rgba(0, 0, 0, 0.7);
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 600;
  color: #fff;
  left: 32rpx;
  top: 32rpx;
  z-index: 100;
}
.auth_layer{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 140;
}
.auth_content{
  position: absolute;
  width: 560rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  background-color: #fff;
  border-radius: 16rpx;
  padding: 70rpx 48rpx 48rpx;
  box-sizing: border-box;
}
.auth_txt{
  font-size: 32rpx;
  font-weight: 400;
  color: #141A27;
  text-align: center;
  margin-bottom: 70rpx;
}
.auth_btn_group{
  display: flex;
  justify-content: space-between;
}
.auth_exit_btn, .auth_setting_btn{
  width: 216rpx;
  height: 96rpx;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
}
.auth_exit_btn{
  background-color: #F0F0F3;
  color: #444253;
  font-weight: 400;
}
.auth_setting_btn{
  color: #fff;
  background-color: #C4A872;
  font-weight: 600;
}
.search_main{
  width: 100%;
  height: 790rpx;
  margin-top: 36rpx;
  overflow-y: scroll;
  box-sizing: border-box;
  padding: 0 32rpx;
}
.search_res_content{
  padding: 16rpx 0;
  border-bottom: 2rpx solid rgba(240, 240, 243, 0.5);
}
.search_item{
  border: none;
}
.search_item .shop_info{
  width: 478rpx;
}
.search_item .shop_title{
  margin-bottom: 0;
}
.search_res_title{
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(68, 66, 83, 0.5);
  margin-bottom: 16rpx;
}
.floor_judge{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.floor_judge_title{
  text-align: center;
  font-size: 48rpx;
  font-weight: 600;
  color: #FFFFFF;
  margin-bottom: 90rpx;
}
.floor_judge_floor{
  text-align: center;
  font-size: 144rpx;
  font-weight: bold;
  color: #FFFFFF;
  margin-bottom: 100rpx;
}
.floor_judge_btn{
  width: 480rpx;
  height: 96rpx;
  background-color: #fff;
  border-radius: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-weight: 600;
  color: #141A27;
  margin: 0 auto 32rpx;
}